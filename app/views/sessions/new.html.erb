<section class="container">
<%= form_for :session, url: session_path, html: { class: "no-info" } do |form| %>
    <div>
      <%= form.label :email %>
      <%= form.email_field :email %>
    </div>
    <div>
      <%= form.label :password %>
      <%= form.password_field :password %>
    </div>
    <div>
      <%= form.submit "Sign in" %>
    </div>
  <% end %>
</section>
